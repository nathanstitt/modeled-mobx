{
  "name": "modeled-mobx",
  "version": "0.0.1",
  "description": "Mobx powered models",
  "author": "nathanstitt",
  "license": "MIT",
  "repository": "nathanstitt/modeled-mobx",
  "main": "dist/modeled-mobx.js",
  "module": "dist/modeled-mobx.module.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "exports": "./dist/modeled-mobx.modern.js",
  "unpkg": "dist/modeled-mobx.umd.js",
  "scripts": {
    "build": "microbundle --no-compress",
    "build:tsc": "tsc",
    "build:tsc:watch": "tsc --watch",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test:unit": "jest",
    "test": "run-s test:unit lint:js test:build",
    "test:build": "run-s build",
    "lint:js": "eslint src/**/*",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "deploy:demo": "run-s predeploy deploy"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/preset-env": "^7.13.5",
    "@babel/preset-typescript": "^7.13.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.12.38",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-argosity": "3.0.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "typescript": "^4.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "mobx": "^6.0"
  }
}
